<p id="notice"><%= notice %></p>

<h1>Warehouses</h1>

<table>
  <thead>
    <tr>
      <th>Wh code</th>
      <th>Name</th>
      <th>Pincode</th>
      <th>Max capacity</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @warehouses.each do |warehouse| %>
      <tr>
        <td><%= warehouse.wh_code %></td>
        <td><%= warehouse.name %></td>
        <td><%= warehouse.pincode %></td>
        <td><%= warehouse.max_capacity %></td>
        <td><%= link_to 'Show', warehouse %></td>
        <td><%= link_to 'Edit', edit_warehouse_path(warehouse) %></td>
        <td><%= link_to 'Destroy', warehouse, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Warehouse', new_warehouse_path %>
